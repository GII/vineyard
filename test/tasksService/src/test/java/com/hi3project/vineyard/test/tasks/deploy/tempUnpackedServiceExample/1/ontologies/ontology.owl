Prefix(xsd:=<http://www.w3.org/2001/XMLSchema#>)
Prefix(owl:=<http://www.w3.org/2002/07/owl#>)
Prefix(TasksModel:=<http://hi3project.com/broccoli/test/tasks#>)
Prefix(xml:=<http://www.w3.org/XML/1998/namespace>)
Prefix(rdf:=<http://www.w3.org/1999/02/22-rdf-syntax-ns#>)
Prefix(rdfs:=<http://www.w3.org/2000/01/rdf-schema#>)
Prefix(process:=<http://www.daml.org/services/owl-s/1.2/Profile.owl#>)
Prefix(service:=<http://www.daml.org/services/owl-s/1.2/Service.owl#>)


Ontology(<http://hi3project.com/broccoli/test/tasks>
Import(<http://www.daml.org/services/owl-s/1.2/Profile.owl>)
Import(<http://www.daml.org/services/owl-s/1.2/Service.owl>)
Annotation(rdfs:comment "    
    A first alpha version of a simple task management ontology, to be used by clients and service providers
  ")
Annotation(rdfs:label "
    Tasks planning and management ontology
  ")

Declaration(Class(TasksModel:ListOfParticipants))
SubClassOf(TasksModel:ListOfParticipants TasksModel:Thing)
SubClassOf(TasksModel:ListOfParticipants ObjectExactCardinality(1 TasksModel:participantsOfList))
Declaration(Class(TasksModel:ListOfTasks))
SubClassOf(TasksModel:ListOfTasks TasksModel:Thing)
SubClassOf(TasksModel:ListOfTasks ObjectExactCardinality(1 TasksModel:taskOfList))
Declaration(Class(TasksModel:NamedThing))
SubClassOf(TasksModel:NamedThing TasksModel:Thing)
Declaration(Class(TasksModel:Participant))
SubClassOf(TasksModel:Participant TasksModel:NamedThing)
Declaration(Class(TasksModel:ParticipationAssignment))
SubClassOf(TasksModel:ParticipationAssignment TasksModel:Thing)
SubClassOf(TasksModel:ParticipationAssignment ObjectExactCardinality(1 TasksModel:participationFromParticipant))
SubClassOf(TasksModel:ParticipationAssignment ObjectExactCardinality(1 TasksModel:participationFromTask))
Declaration(Class(TasksModel:Project))
SubClassOf(TasksModel:Project TasksModel:NamedThing)
Declaration(Class(TasksModel:Role))
SubClassOf(TasksModel:Role TasksModel:NamedThing)
Declaration(Class(TasksModel:Task))
SubClassOf(TasksModel:Task TasksModel:NamedThing)
Declaration(ObjectProperty(TasksModel:participantsOfList))
ObjectPropertyDomain(TasksModel:participantsOfList TasksModel:ListOfParticipants)
ObjectPropertyRange(TasksModel:participantsOfList TasksModel:Task)
Declaration(ObjectProperty(TasksModel:participationFromParticipant))
ObjectPropertyDomain(TasksModel:participationFromParticipant TasksModel:ParticipationAssignment)
ObjectPropertyRange(TasksModel:participationFromParticipant TasksModel:Participant)
Declaration(ObjectProperty(TasksModel:participationFromRole))
ObjectPropertyDomain(TasksModel:participationFromRole TasksModel:ParticipationAssignment)
ObjectPropertyRange(TasksModel:participationFromRole TasksModel:Role)
Declaration(ObjectProperty(TasksModel:participationFromTask))
ObjectPropertyDomain(TasksModel:participationFromTask TasksModel:ParticipationAssignment)
ObjectPropertyRange(TasksModel:participationFromTask TasksModel:Task)
Declaration(ObjectProperty(TasksModel:roles))
ObjectPropertyDomain(TasksModel:roles TasksModel:Task)
ObjectPropertyRange(TasksModel:roles TasksModel:Role)
Declaration(ObjectProperty(TasksModel:taskFromProject))
ObjectPropertyDomain(TasksModel:taskFromProject TasksModel:Task)
ObjectPropertyRange(TasksModel:taskFromProject TasksModel:Project)

Declaration(ObjectProperty(TasksModel:tasksFromProject))
ObjectPropertyDomain(TasksModel:tasksFromProject TasksModel:Project)
ObjectPropertyRange(TasksModel:tasksFromProject TasksModel:Task)

Declaration(ObjectProperty(TasksModel:taskOfList))
ObjectPropertyDomain(TasksModel:taskOfList TasksModel:ListOfTask)
ObjectPropertyRange(TasksModel:taskOfList TasksModel:Task)
Declaration(DataProperty(TasksModel:nameOF))
DataPropertyDomain(TasksModel:nameOF TasksModel:NamedThing)
DataPropertyRange(TasksModel:nameOF xsd:string)
Declaration(DataProperty(TasksModel:nameP))
DataPropertyDomain(TasksModel:nameP TasksModel:Participant)
DataPropertyRange(TasksModel:nameP xsd:string)
)
